{
  "version": "0.2.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@prequest/use-request",
  "author": "Thomas Trainset",
  "module": "dist/use-request.esm.js",
  "size-limit": [
    {
      "path": "dist/use-request.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/use-request.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@xdoer/timeout-interval": "^0.1.2"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "husky": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  }
}
